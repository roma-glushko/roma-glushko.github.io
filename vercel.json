{
    "trailingSlash": true,
    "redirects": [
        {"source": "/lab/", "destination": "/blog/", "statusCode": 301},
        {"source": "/sitemap/sitemap-index.xml ", "destination": "/sitemap-index.xml", "statusCode": 301}
    ],
    "rewrites": [
        {
            "source": "/lab/:match*",
            "destination": "/blog/:match*",
            "permanent": true
        }
    ]
}